    				4 = Birth certificates
    				5 = Legal guardianship requirements
    				6 = Transportation
    				7 = Lack of available preschool programs
    				8 = Don’t know
    				9 = Refused
    				10 = Immunization requirements
    				11 = Physical examination records
    				12 = Other
			</xsd:documentation>
		</xsd:annotation>
