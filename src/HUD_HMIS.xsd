		<!-- Checking that that the Project Type = ‘Emergency shelter’ for certain 2.07 data elements to exist -->
		<xs:assert test="every $inventory in hmis:Inventory satisfies (if (boolean($inventory/hmis:Availability) or boolean($inventory/hmis:ESBedType)) then (if (every $id in $inventory/hmis:ProjectID satisfies (hmis:Project[hmis:ProjectID = $id]/hmis:ProjectType/text()='1')) then (true()) else false()) else (true()))"/>
		<!-- Checking that that the Project Type = ‘Emergency shelter’ for certain 2.07 data elements to exist -->
		<xs:assert test="every $inventory in hmis:Inventory satisfies (if (boolean($inventory/hmis:Availability) or boolean($inventory/hmis:ESBedType)) then (if (every $id in $inventory/hmis:ProjectID satisfies (hmis:Project[hmis:ProjectID = $id]/hmis:ProjectType/text()='1')) then (true()) else false()) else (true()))"/>	
		<!-- Checking that that the CurrentSchoolAttend = ‘Not currently enrolled in any school or educational course’ for data element MostRecentEdStatus to exist -->
		<xs:assert test="every $youthEducationStatus in hmis:YouthEducationStatus satisfies (  
			if (boolean($youthEducationStatus/hmis:MostRecentEdStatus)) 
