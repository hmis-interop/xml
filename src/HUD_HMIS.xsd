		<xs:assert test="if (hmis:Event/text()='5') then boolean(hmis:ReferralCaseManageAfter) else (if (not(hmis:ReferralCaseManageAfter/text()='5')) then (not(boolean(hmis:ReferralCaseManageAfter))) else false())"/>
