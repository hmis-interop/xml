	<xs:complexType name="client">
		<xs:sequence>
			<xs:element name="PersonalID" type="hmis:personalID"/>
			<xs:element name="FirstName" id="_3.01.1" type="hmis:nameHashingOption" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">First</xs:documentation>
				</xs:annotation>
				<xs:alternative test="@hmis:hashStatus='1'" type="hmis:nameHashingOptionPlain"/>
				<xs:alternative test="@hmis:hashStatus='2'" type="hmis:nameHashingOptionSHA1"/>
				<xs:alternative test="@hmis:hashStatus='3'" type="hmis:nameHashingOption"/>
				<xs:alternative test="@hmis:hashStatus='4'" type="hmis:nameHashingOptionSHA256"/>
			</xs:element>
			<xs:element name="MiddleName"  id="_3.01.2" type="hmis:nameHashingOption" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Middle</xs:documentation>
				</xs:annotation>
				<xs:alternative test="@hmis:hashStatus='1'" type="hmis:nameHashingOptionPlain"/>
				<xs:alternative test="@hmis:hashStatus='2'" type="hmis:nameHashingOptionSHA1"/>
				<xs:alternative test="@hmis:hashStatus='3'" type="hmis:nameHashingOption"/>
				<xs:alternative test="@hmis:hashStatus='4'" type="hmis:nameHashingOptionSHA256"/>
			</xs:element>
			<xs:element name="LastName" id="_3.01.3" type="hmis:nameHashingOption" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Last</xs:documentation>
				</xs:annotation>
				<xs:alternative test="@hmis:hashStatus='1'" type="hmis:nameHashingOptionPlain"/>
				<xs:alternative test="@hmis:hashStatus='2'" type="hmis:nameHashingOptionSHA1"/>
				<xs:alternative test="@hmis:hashStatus='3'" type="hmis:nameHashingOption"/>
				<xs:alternative test="@hmis:hashStatus='4'" type="hmis:nameHashingOptionSHA256"/>				
			</xs:element>
			<xs:element name="NameSuffix"  id="_3.01.4" type="hmis:string50" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Suffix</xs:documentation>					
				</xs:annotation>
			</xs:element>
			<xs:element name="NameDataQuality"  id="_3.01.5" type="hmis:nameDataQuality">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name Data Quality</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSN"  id="_3.02.1" type="hmis:ssnHashingOption" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Social Security Number</xs:documentation>
					<xs:documentation xml:lang="en">Unknown SSN digits should be indicated with a lower case "x".</xs:documentation>
				</xs:annotation>
				<xs:alternative test="@hmis:hashStatus='1'" type="hmis:ssnHashingOptionPlain"/>
				<xs:alternative test="@hmis:hashStatus='2'" type="hmis:ssnHashingOptionSHA1"/>
				<xs:alternative test="@hmis:hashStatus='3'" type="hmis:ssnHashingOption"/>
				<xs:alternative test="@hmis:hashStatus='4'" type="hmis:ssnHashingOptionSHA256"/>				
			</xs:element>
			<xs:element name="SSNDataQuality"  id="_3.02.2" type="hmis:ssnDataQuality">
				<xs:annotation>
					<xs:documentation xml:lang="en">SSN Data Quality</xs:documentation>
				</xs:annotation>
			</xs:element>			
			<xs:element name="DOB"  id="_3.03.1" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Date of Birth</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOBDataQuality"   id="_3.03.2" type="hmis:dobDataQuality">
				<xs:annotation>
					<xs:documentation xml:lang="en">DOB Data Quality</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmIndAKNative" id="_3.04.1" type="noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">American Indian, Alaska Native, or Indigenous</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Asian" id="_3.04.2" type="noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Asian or Asian American</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BlackAfAmerican" id="_3.04.3" type="noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Black, African American, or African</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NativeHIOtherPacific" id="_3.04.4" type="noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Native Hawaiian or Pacific Islander</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="White" id="_3.04.5" type="noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">White</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RaceNone" id="_3.04.6" type="hmis:raceGenderNone" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Race None</xs:documentation>
				</xs:annotation>
			</xs:element>			
			<xs:element name="Ethnicity" id="_3.05.1" type="hmis:ethnicity">
				<xs:annotation>
					<xs:documentation xml:lang="en">Ethnicity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Female" id="_3.06.1.1" type="hmis:noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Female</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Male" id="_3.06.1.2" type="hmis:noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Male</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GenderOther" id="_3.06.1.3" type="hmis:noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">A gender other than singularly female or male (e.g., non-binary, genderfluid, agender, culturally specific gender)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Transgender" id="_3.06.1.4" type="hmis:noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Transgender</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Questioning" id="_3.06.1.5" type="hmis:noYesMissing">
				<xs:annotation>
					<xs:documentation xml:lang="en">Questioning</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GenderNone" id="_3.06.1.6" type="hmis:raceGenderNone" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Gender None</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VeteranStatus" id="_3.07.1" type="hmis:noYesDoesntKnowRefused">
				<xs:annotation>
					<xs:documentation xml:lang="en">Veteran Status</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="hmis:baseMetaData"/>
		<!--Testing if all genders are 0 or 99, before requiring use of GenderNone-->
		<xs:assert test="
			if (hmis:GenderNone) 
				then ((hmis:Female/text()='0' or hmis:Female/text()='99') and (hmis:Male/text()='0' or hmis:Male/text()='99') and (hmis:GenderOther/text()='0' or hmis:GenderOther/text()='99') and (hmis:Transgender/text()='0' or hmis:Transgender/text()='99') and (hmis:Questioning/text()='0' or hmis:Questioning/text()='99'))
			else 
				if (hmis:Female/text()='1' or hmis:Male/text()='1' or hmis:GenderOther/text()='1' or hmis:Transgender/text()='1' or hmis:Questioning/text()='1')
					then true()
				else false() "/>
		<!--Testing if all race values are are 0 or 99, before requiring use of RaceNone-->
		<xs:assert test="
			if (hmis:RaceNone) 
				then ((hmis:AmIndAKNative/text()='0' or hmis:AmIndAKNative/text()='99') and (hmis:Asian/text()='0' or hmis:Asian/text()='99') and (hmis:BlackAfAmerican/text()='0' or hmis:BlackAfAmerican/text()='99') and (hmis:NativeHIOtherPacific/text()='0' or hmis:NativeHIOtherPacific/text()='99') and (hmis:White/text()='0' or hmis:White/text()='99')) 
			else 
				if (hmis:AmIndAKNative/text()='1' or hmis:Asian/text()='1' or hmis:BlackAfAmerican/text()='1' or hmis:NativeHIOtherPacific/text()='1' or hmis:White/text()='1')
					then true()
				else false() "/>
		
	</xs:complexType>
	
