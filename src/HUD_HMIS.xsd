		<xs:assert test="if (hmis:Event/text()ge'10' and hmis:Event/text()le'15') then boolean(hmis:LocationCrisisOrPHHousing) else (if (not(hmis:Event/text()ge'10' and hmis:Event/text()le'15')) then (not(boolean(hmis:LocationCrisisOrPHHousing))) else false())"/>
		<!--Testing if Event is 10-15, before allowing use of ReferralResult-->
		<xs:assert test="if (hmis:Event/text()ge'10' and hmis:Event/text()le'15') then boolean(hmis:ReferralResult) else (if (not(hmis:Event/text()ge'10' and hmis:Event/text()le'15')) then (not(boolean(hmis:ReferralResult))) else false())"/>
		<!--Testing if ReferralResult is not null, before allowing use of ResultDate-->
		<xs:assert test="if (hmis:ReferralResult) then boolean(hmis:ResultDate) else (if (not(hmis:ReferralResult)) then (not(boolean(hmis:ResultDate))) else false())"/>		
	</xs:complexType>

