		<!--Testing if the RecordType is '300' and the TypeProvided is '5', before requiring and even allowing the use of MovingOnOtherType -->
		<xs:assert test="if (not (hmis:RecordType/text() ='161')) then (not (boolean(hmis:ReferralOutcome))) else boolean(hmis:ReferralOutcome)"/>
