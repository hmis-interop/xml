		<xs:assert test="every $youthEducationStatus in hmis:YouthEducationStatus satisfies (  
			if (boolean($youthEducationStatus/hmis:CurrentEdStatus)) 
				then (if ($youthEducationStatus/hmis:CurrentSchoolAttend/text()='1' or $youthEducationStatus/hmis:CurrentSchoolAttend/text()='2') then true() else false())
			else true()
			)"
		/>
